{
  "name": "obsidian-plugin-ts",
  "version": "1.0.0",
  "description": "A modular template for building Obsidian plugins with TypeScript",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm:watch\" \"pnpm:dev-copy\" -n ROLLUP,COPY -c blue,green",
    "prewatch": "node scripts/generate-manifest.mjs",
    "watch": "rollup --config rollup.config.mjs -w",
    "dev-copy": "node scripts/dev-copy.mjs",
    "dev:setup": "node scripts/dev-setup.mjs",
    "build": "rollup --config rollup.config.mjs && pnpm postbuild",
    "build:prod": "cross-env BUILD=production rollup --config rollup.config.mjs && pnpm postbuild",
    "postbuild": "node scripts/generate-manifest.mjs && node scripts/generate-version.mjs",
    "version": "node scripts/version-bump.mjs && git add src/shared/lib/manifest.ts versions.json",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "template"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^12.3.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.9.2",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "jsdom": "^25.0.1",
    "obsidian": "latest",
    "postcss": "^8.5.6",
    "rollup": "^4.52.5",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.4.18",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  }
}
