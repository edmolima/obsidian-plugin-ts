{
  "name": "obsidian-plugin-ts",
  "version": "1.0.0",
  "description": "A modular template for building Obsidian plugins with TypeScript",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
  "dev": "concurrently \"pnpm:watch\" \"pnpm:dev-copy\" -n ROLLUP,COPY -c blue,green",
  "prewatch": "node scripts/generate-manifest.mjs",
  "watch": "rollup --config rollup.config.mjs -w",
  "dev-copy": "node scripts/dev-copy.mjs",
  "dev:setup": "node scripts/dev-setup.mjs",
  "build": "rollup --config rollup.config.mjs && pnpm postbuild",
  "build:prod": "cross-env BUILD=production rollup --config rollup.config.mjs && pnpm postbuild",
  "postbuild": "node scripts/generate-manifest.mjs && node scripts/generate-version.mjs",
    "version": "node scripts/version-bump.mjs && git add src/shared/lib/manifest.ts versions.json",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "template"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "jsdom": "^25.0.1",
    "obsidian": "latest",
    "postcss": "^8.4.49",
    "rollup": "^4.28.1",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3"
  }
}
